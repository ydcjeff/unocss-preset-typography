/* layer: typography */
.prose :where(h1, h2, h3, h4, h5, h6):not(.not-prose) {
  color: var(--uno-prose-headings);
  font-weight: 600;
  line-height: 1.25;
}
.prose :where(a):not(.not-prose) {
  color: var(--uno-prose-links);
  text-decoration: underline;
  font-weight: 500;
}
.prose :where(a code):not(.not-prose) {
  color: var(--uno-prose-links);
}
.prose :where(p, ul, ol, pre):not(.not-prose) {
  margin: 1em 0;
  line-height: 1.75;
}
.prose :where(blockquote):not(.not-prose) {
  margin: 1em 0;
  padding-left: 1em;
  font-style: italic;
  border-left: 0.25em solid var(--uno-prose-borders);
}
.prose :where(h1):not(.not-prose) {
  margin: 1rem 0;
  font-size: 2.25em;
}
.prose :where(h2):not(.not-prose) {
  margin: 1.75em 0 0.5em;
  font-size: 1.75em;
}
.prose :where(h3):not(.not-prose) {
  margin: 1.5em 0 0.5em;
  font-size: 1.375em;
}
.prose :where(h4):not(.not-prose) {
  margin: 1em 0;
  font-size: 1.125em;
}
.prose :where(img, video):not(.not-prose) {
  max-width: 100%;
}
.prose :where(figure, picture):not(.not-prose) {
  margin: 1em 0;
}
.prose :where(figcaption):not(.not-prose) {
  color: var(--uno-prose-captions);
  font-size: 0.875em;
}
.prose :where(code):not(.not-prose) {
  color: var(--uno-prose-code);
  font-size: 0.875em;
  font-weight: 600;
  font-family: ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas,
    Liberation-Mono, Courier-New, monospace;
}
.prose :where(:not(pre) > code):not(.not-prose)::before,
.prose :where(:not(pre) > code):not(.not-prose)::after {
  content: '`';
}
.prose :where(pre):not(.not-prose) {
  padding: 1.25rem 1.5rem;
  overflow-x: auto;
  border-radius: 0.375rem;
}
.prose :where(pre, code):not(.not-prose) {
  white-space: pre;
  word-spacing: normal;
  word-break: normal;
  word-wrap: normal;
  -moz-tab-size: 4;
  -o-tab-size: 4;
  tab-size: 4;
  -webkit-hyphens: none;
  -moz-hyphens: none;
  hyphens: none;
  background: transparent;
}
.prose :where(pre code):not(.not-prose) {
  font-weight: inherit;
}
.prose :where(ol, ul):not(.not-prose) {
  padding-left: 1.25em;
}
.prose :where(ul):not(.not-prose) {
  list-style-type: disc;
}
.prose :where(ol > li):not(.not-prose)::marker,
.prose :where(ul > li):not(.not-prose)::marker,
.prose :where(summary):not(.not-prose)::marker {
  color: var(--uno-prose-lists);
}
.prose :where(hr):not(.not-prose) {
  margin: 2em 0;
  border: 1px solid var(--uno-prose-hr);
}
.prose :where(table):not(.not-prose) {
  display: block;
  margin: 1em 0;
  border-collapse: collapse;
  overflow-x: auto;
}
.prose :where(tr):not(.not-prose):nth-child(2n) {
  background: var(--uno-prose-bg-soft);
}
.prose :where(td, th):not(.not-prose) {
  border: 1px solid var(--uno-prose-borders);
  padding: 0.625em 1em;
}
.prose :where(abbr):not(.not-prose) {
  cursor: help;
}
.prose :where(kbd):not(.not-prose) {
  color: var(--uno-prose-code);
  border: 1px solid;
  padding: 0.25rem 0.5rem;
  font-size: 0.875em;
  border-radius: 0.25rem;
}
.prose :where(details):not(.not-prose) {
  margin: 1em 0;
  padding: 1.25rem 1.5rem;
  background: var(--uno-prose-bg-soft);
  border-left: 0.5em solid;
}
.prose :where(summary):not(.not-prose) {
  cursor: pointer;
  font-weight: 600;
}
.prose {
  color: var(--uno-prose-body);
  max-width: 65ch;
}
.prose-teal {
  --uno-prose-body: #0f766e;
  --uno-prose-headings: #134e4a;
  --uno-prose-links: #134e4a;
  --uno-prose-lists: #2dd4bf;
  --uno-prose-hr: #99f6e4;
  --uno-prose-captions: #14b8a6;
  --uno-prose-code: #134e4a;
  --uno-prose-borders: #99f6e4;
  --uno-prose-bg-soft: #ccfbf1;
  --uno-prose-invert-body: #99f6e4;
  --uno-prose-invert-headings: #ccfbf1;
  --uno-prose-invert-links: #ccfbf1;
  --uno-prose-invert-lists: #14b8a6;
  --uno-prose-invert-hr: #0f766e;
  --uno-prose-invert-captions: #2dd4bf;
  --uno-prose-invert-code: #ccfbf1;
  --uno-prose-invert-borders: #0f766e;
  --uno-prose-invert-bg-soft: #115e59;
}
.dark .dark\:prose-invert {
  --uno-prose-body: var(--uno-prose-invert-body);
  --uno-prose-headings: var(--uno-prose-invert-headings);
  --uno-prose-links: var(--uno-prose-invert-links);
  --uno-prose-lists: var(--uno-prose-invert-lists);
  --uno-prose-hr: var(--uno-prose-invert-hr);
  --uno-prose-captions: var(--uno-prose-invert-captions);
  --uno-prose-code: var(--uno-prose-invert-code);
  --uno-prose-borders: var(--uno-prose-invert-borders);
  --uno-prose-bg-soft: var(--uno-prose-invert-bg-soft);
}
/* layer: default */
.text-base {
  font-size: 1rem;
  line-height: 1.5rem;
}
